# LENS-EXEC Configuration for Gemini API
# Copy this file and add your actual API key

# Gemini API Configuration
VITE_LLM_API_KEY=your_gemini_api_key_here
VITE_LLM_MODEL=gemini-1.5-pro
VITE_LLM_MAX_TOKENS=2048
VITE_LLM_TEMPERATURE=0.1
VITE_LLM_TIMEOUT=30000

# Camera Configuration (optional)
VITE_CAMERA_WIDTH=1280
VITE_CAMERA_HEIGHT=720
VITE_CAMERA_FRAME_RATE=30
VITE_CAMERA_FACING_MODE=environment

# Debug Mode
VITE_DEBUG_MODE=true